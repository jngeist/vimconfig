hi ModeMsg	guifg=fg	guibg=bg
hi LineNr	guifg=#804040	guibg=bg
hi Question	guifg=#2e8b57	guibg=#d0d090
hi StatusLineNC	guifg=bg	guibg=fg
hi VertSplit	guifg=bg	guibg=fg
hi Title	guifg=#ff00ff	guibg=bg
hi Visual	guifg=fg	guibg=#c0c0c0
hi VisualNOS	guifg=fg	guibg=bg	gui=underline
hi WarningMsg	guifg=#ff0000	guibg=bg
hi WildMenu	guifg=fg	guibg=#ffff00
hi Folded	guifg=#000080	guibg=#e0e0e0
hi FoldColumn	guifg=#000080	guibg=#c0c0c0
hi DiffAdd	guifg=fg	guibg=#a0a0ff
hi DiffChange	guifg=fg	guibg=#ffa0ff
hi DiffDelete	guifg=#0000ff	guibg=#a0ffff
hi DiffText	guifg=fg	guibg=#ff0000
hi SignColumn	guifg=#000080	guibg=#c0c0c0
hi Cursor	guifg=bg	guibg=fg

hi Underlined	guifg=#6a5acd	guibg=bg	gui=underline 
hi Ignore	guifg=bg	guibg=bg
hi Error	guifg=#ffffff	guibg=#ff0000
hi Todo		guifg=#0000ff	guibg=#ffff00
hi PreProc	guifg=#a020f0	guibg=bg
hi MoreMsg	guifg=#2e8b57	guibg=bg
hi IncSearch	guifg=bg	guibg=fg
hi Constant	guifg=#ff00ff	guibg=bg
hi Search	guifg=fg	guibg=#ffff00
hi Statement	guifg=#804040	guibg=bg
hi Type		guifg=#2e8b57	guibg=bg
hi StatusLine	guifg=bg	guibg=fg
hi Comment	guifg=#0000ff	guibg=bg
hi Special	guifg=#80a0ff	guibg=bg
hi Identifier	guifg=#008080	guibg=bg


" LIGHT COLOR DEFINE END

" COLOR LINKS DEFINE START

echo "Other highlight statements can be put here, will be executed unparsed."


" COLOR LINKS DEFINE END

